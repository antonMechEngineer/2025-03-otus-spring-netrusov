{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "buy": {
      "type": "string"
    },
    "buyId": {
      "type": "integer"
    },
    "price": {
      "type": "number"
    },
    "username": {
      "type": "string"
    },
    "actionType": {
      "type": "string",
      "enum": ["PAY", "CANCEL"]
    }
  },
  "required": ["buy", "buyId", "price", "username", "actionType"],
  "additionalProperties": false
}